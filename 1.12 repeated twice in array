#include <stdio.h>
#include <stdlib.h>
void Repeating(int arr[], int size)
{
    int i, j;
    printf("VIJAYALAKSHMI G,192321080\n");
    printf("Repeating elements are:\n");
    for (i = 0; i < size - 1; i++)
        for (j = i + 1; j < size; j++)
            if (arr[i] == arr[j])
                printf("%d ", arr[i]);
}

int main()
{
    int arr[] = { 4, 2, 4, 5, 2, 3, 1 };
    int arr_size = sizeof(arr) / sizeof(arr[0]);
    Repeating(arr, arr_size);
    getchar();
    return 0;
}
