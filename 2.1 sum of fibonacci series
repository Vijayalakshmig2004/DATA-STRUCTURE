#include<stdio.h>
int main()
{
   int a=0, b=1, range, c, sum=0;
   printf("vijayalakshmi g,192321080\n");
   printf("Enter the range of Fibonacci series:\n");
   scanf("%d",&range);
   printf("The fibonacci series is:");
   while( a<=range )
   {
      printf("%d\t",a);
      sum+=a;
      c=a+b;
      a=b;
      b=c;
   }
   printf("\n sum is = %d",sum);
   return 0;
}
